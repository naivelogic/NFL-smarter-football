    "## Team Stats"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 29,
   "metadata": {},
   "outputs": [],
   "source": [
    "# indicate offensive plays\n",
    "offensive_plays = ['pass', 'run']\n",
    "nfl.loc[nfl.play_type.isin(offensive_plays), 'off_plays'] = 1\n",
    "nfl.loc[nfl.desc.str.contains(' sacked'), 'off_plays'] = 1\n",
    "nfl.loc[nfl.desc.str.contains(' sacked'), 'sack'] = 1\n",
    "nfl['off_play_yds'] = np.where(nfl.off_plays== 1, nfl.yards_gained, 0)\n",
    "\n",
    "nfl['first_down'] = nfl['first_down_rush'] + nfl['first_down_pass'] + nfl['first_down_penalty']\n",
    "nfl.loc[(nfl.pass_touchdown == 1) | (nfl.rush_touchdown == 1) | (nfl.field_goal_result == 'made'), 'off_score_drive'] = 1\n",
    "\n",
    "# indicate play attempt\n",
    "nfl.loc[(nfl.pass_attempt == 1) | (nfl.rush_attempt == 1), 'play_attempt'] = 1"
   ]
  },
